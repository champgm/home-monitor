{
  "extends": "tslint-config-airbnb",
  "rules": {
    "max-line-length": [
      true,
      120
    ],
    "import-name": false,
    "strictNullChecks": false,
    "whitespace": false,
    "quotemark": [
      true,
      "single"
    ],
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "ordered-imports": true,
    "object-literal-sort-keys": [
      true,
      "ignore-case",
      "shorthand-first"
    ],
    "no-unused-variable": true
  }
}